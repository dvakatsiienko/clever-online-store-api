{
    "name": "clever-online-store-api",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "start": "keystone-next",
        "build": "keystone-next build",
        "serve": "keystone-next start",
        "seed-data": "keystone-next --seed-data",
        "check-all": "run-p check:*",
        "check:ts": "tsc --project ./tsconfig.json",
        "prettify": "prettier-eslint --write \"$PWD/src/**/*.{ts,tsx}\"",
        "check:eslint": "eslint --max-warnings 10 ./src",
        "prepare": "husky install"
    },
    "dependencies": {
        "@keystone-next/admin-ui": "12.0.1",
        "@keystone-next/auth": "19.0.0",
        "@keystone-next/cloudinary": "3.0.4",
        "@keystone-next/fields": "5.3.0",
        "@keystone-next/keystone": "14.0.0",
        "@keystone-next/types": "15.0.0",
        "@keystonejs/server-side-graphql-client": "2.1.0",
        "dotenv": "8.2.0",
        "next": "10.0.9",
        "nodemailer": "6.5.0",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "stripe": "8.140.0"
    },
    "devDependencies": {
        "@types/dotenv": "8.2.0",
        "@types/nodemailer": "6.4.1",
        "@typescript-eslint/eslint-plugin": "4.19.0",
        "@typescript-eslint/parser": "4.19.0",
        "babel-eslint": "10.1.0",
        "eslint": "7.22.0",
        "eslint-config-airbnb": "18.2.1",
        "eslint-config-dva": "0.0.5",
        "eslint-plugin-align-import": "1.0.0",
        "eslint-plugin-import": "2.22.1",
        "eslint-plugin-jsx-a11y": "6.4.1",
        "eslint-plugin-react": "7.23.1",
        "eslint-plugin-react-hooks": "4.2.0",
        "husky": "5.2.0",
        "npm-run-all": "4.1.5",
        "prettier": "2.2.1",
        "prettier-eslint-cli": "5.0.1",
        "tsconfig-dva": "0.0.3",
        "typescript": "4.2.3"
    }
}
